pragma ton-solidity >= 0.35.0;
pragma AbiHeader expire;

contract Calculator {


    constructor() public 
    {
        require(tvm.pubkey() != 0, 101);
        require(msg.pubkey() == tvm.pubkey(), 102);
        tvm.accept();
    }

    function add (int x, int y) public pure returns (int) {
        require(msg.pubkey() == tvm.pubkey(), 102);
        tvm.accept();
        return x + y;
    }
}
